package no.morganbp.oblig1;

public class MainClass {

	public static void main(String[] args) {
		int[] a = { 1, 2, 2, 2, 4, 2, 2, 4, 4, 2, 1, 4, 3, 2, 1, 5 };
		int[] b = { 1, 2, 2, 2, 4, 2, 2, 4, 4, 2, 1, 4, 3, 2, 1, 5 };
		int index;
		int n = 16;
		int col = 0;
		int row = 0;
		int nsqr = (int)Math.sqrt(n);
		System.out.println(Math.sqrt(n));
		// int[] c = MatrixMultiply.squareMatrixMultiplyRecursive(a,b);
		for (int i = 0; i < n; i++) {
			index = (int) ((i % nsqr + ((i / Math.sqrt(n))* (n/2))) + (col * (n / 4)) + ((2 * row * n)));
			System.out.println(index);
		}
	}

}
